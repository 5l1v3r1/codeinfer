      </AnchorPane>
      <Pane prefHeight="236.0" prefWidth="528.0">
         <children>
            <Label fx:id="ErrorMessageLabel" alignment="TOP_RIGHT" contentDisplay="RIGHT" layoutX="20.0" layoutY="83.0" nodeOrientation="RIGHT_TO_LEFT" prefHeight="28.0" prefWidth="487.0" text="An error occured when trying to parse" textFill="WHITE">
